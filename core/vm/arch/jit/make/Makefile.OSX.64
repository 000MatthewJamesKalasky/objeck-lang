# ARGS=-g -D_DEBUG  -Wunused -Wall -std=c++11 -Wno-unused-function
ARGS=-O3 -Wall -Wno-unused-function -std=c++11

CC=g++
AR=ar
SRC=jit_common.o
LIB=amd64/jit_common.a

$(LIB): $(SRC)
	$(AR) -cvq $(LIB) $(SRC) -L../lib/openssl/macos/x64 $(OBJ_LIBS) -lssl -lcrypto -ldl -lz

%.o: %.cpp
	$(CC) -m64 $(ARGS) -c $< 

clean:
	rm -f *.o *~ $(LIB)
