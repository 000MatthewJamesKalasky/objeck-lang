CC=g++
# ARGS=-O3 -Wall -Wno-unused-function -D_X64 -std=c++11
ARGS=-g -Wall -Wno-unused-function -D_X64 -D_DIAG_LIB -std=c++11
SRC=../../compiler/scanner.o ../../compiler/parser.o ../../compiler/context.o ../../compiler/tree.o ../../compiler/types.o diags.o 
EXE=diags

$(EXE): $(OBJ_LIBS) $(SRC) 
	$(CC) -m64 -o $(EXE) $(SRC) -lz

%.o: %.cpp
	$(CC) -m64 $(ARGS) -c $< 

clean:
	rm -f $(EXE) *.o *~

