use System.IO;
use System.API;
use Collection.Generic;
use Web.HTTP;

#~
Support for the IIS web server
~#
bundle Web.Server {
	class Proxy {
		@lib_proxy : static : DllProxy;
		
		function : GetDllProxy() ~ DllProxy {
			if(@lib_proxy = Nil) {
				@lib_proxy := DllProxy->New("libobjk_iis_web_server");
			};

			return @lib_proxy;
		}
	}

	#~
	IIS request
	~#
	class IISRequest implements Web.Server.Request {
		
	}

	#~
	IIS Response 
	~#
	class IISResponse  Web.Server.Response {
		
	}
}
