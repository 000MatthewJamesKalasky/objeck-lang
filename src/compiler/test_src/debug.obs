use CSV;

class Test {
	function : Main(args : String[]) ~ Nil {
		data := 
"C1,C2,C3,C4,C5
31,73,16,5,9
4,8,12,16,20";

		csv := CsvTable->New(data, "\n");
		csv->AppendColumn("sum");
		csv->Size()->PrintLine();
		csv->RowSize()->PrintLine();

		(csv->ColumnValues("C6") <> Nil)->PrintLine();

		each(i : csv) {
			row := csv->Get(i);
	s := row->Sum();	
			row->Set(s->As(Int)->ToString(), 5);
			each(j : row) {
				row->Get(j)->Trim()->Print();
				","->Print();
			};
			"\n"->Print();
		};
		f := csv->ColumnValues("sum");
		f->Sum()->PrintLine();
	}
}
