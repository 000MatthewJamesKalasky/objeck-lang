use Collection;
use CSV;

class Test {

	function : Main(args : String[]) ~ Nil {
		table : CsvTable := CsvTable->New(System.IO.File.FileReader->ReadFile(args[0]), "\r\n");
		AttachRate(table, "10/1/2013");
	}
	
	function : AttachRate(table : CsvTable, quarter : String) ~ Nil {
		fiscal_quarter := table->Search("Product: Fiscal Quarter", quarter);
		fiscal_quarter->Size()->PrintLine();

		contracts := fiscal_quarter->UniqueColumnValues("Contract");
		contracts->Size()->PrintLine();
	}
}
