class Test {
	function : Main(args : String[]) ~ Nil {
		limit := 2000;
		for(n := 1; n < limit+1; n++;) {
			m := SumProp(n);
			if(n = SumProp(m) & n <> m) {
				IO.Console->Print(n)->Print(' ')->PrintLine(m);
			};
		};
	}
	
	function : SumProp(num : Float) ~ Float {
		if (num < 2) {
			return 0;
		};
		
		sum := 1;
		root := num->As(Float)->SquareRoot();
		
		for(i:= 2; i < root; i++;) {
			if(num->As(Int) % i = 0) { 
				sum := sum + i + num/i;
			};
		};
		
		if(num->As(Int) % root->As(Int) = 0) {
			sum := sum + root;
		};
		
		return sum;
	}
}
