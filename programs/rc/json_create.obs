use Collection;
use Data.JSON;

bundle Default {
	class Json {
		function : Main(args : String[]) ~ Nil {
			parser := JsonParser->New("{ \"foo\": 1, \"bar\": [10, \"apples\"] }");
         root := parser->Parse();
         if(root <> Nil) {
				root->ToString()->PrintLine();
			};
		}
	}
}
