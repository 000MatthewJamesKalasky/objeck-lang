use Collection.Generic;

lambdas Func { Def : (IntHolder, String) ~ Nil }

class Test {
	function : Main(args : String[]) ~ Nil {
		map := Map->New()<IntHolder, String>;
		map->Insert(1, "One");
		map->Insert(3, "Three");
		map->Insert(5, "Five");
		map->Each(\Func->Def : (k, v) => "{$k}: {$v}"->PrintLine());
	}
}
