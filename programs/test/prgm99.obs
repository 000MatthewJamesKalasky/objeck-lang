use Collection.Generic;

class Test {
	function : Main(args : String[]) ~ Nil {
		map := Map->New()<IntHolder, String>;
		map->Insert(1, "One");
		map->Insert(3, "Three");
		map->Insert(5, "Five");
		map->Each(Print(IntHolder, String) ~ Nil);
	}
	
	function : Print(k : IntHolder, v : String) ~ Nil {
		"{$k}: {$v}"->PrintLine();
	}
}
