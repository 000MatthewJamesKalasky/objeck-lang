class Test {
	@s : static : String;

	function : Main(args : String[]) ~ Nil {
		Init();
		
		f := Foo->New(@s);
		f->Show();
	}

	function : Init() ~ Nil {
		@s := "Hello World!";
	}
}

class Foo {
	@s : String;

	New(s : String) {
		@s := s;
	}

	method : public : Show() ~ Nil {
		@s->PrintLine();
	}
}