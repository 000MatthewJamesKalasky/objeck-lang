class Fee {
	@fee : String;
	New() {
		Parent();
		@free := "fee";
	}
}

class Fie from Fee {
	@fie : Float;
	New() {
		Parent();
		@free := 33.3;
	}
}

class Fum from Fie {
	@fum : Int;
	New() {
		Parent();
		@free := 13;
	}

	method : public : DoIt() ~ Nil {
		if(@fie < 15.5 & @fum = 13) {
			"Works!"->PrintLine();
		}
		else {
			"Broken"->PrintLine();
		};
	}
}

class Test {
	function : Main(args : String[]) ~ Nil {
		Fum->New()->DoIt();		
	}
}
