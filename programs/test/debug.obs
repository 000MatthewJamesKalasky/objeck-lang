use Collection;

class Node {
	@value0 : static : Int;
	@value1 : Int;
	@value2 : Float;
	@value3 : String;

	New() {
		@value0 += 1;
		@value1 := 13;
		@value2 := 3.145;
		@value3 := "daeo";
	}
}

class Test {
	function : Main(args : String[]) ~ Nil {
		nodes := Vector->New();
		Run(nodes);
		nodes->Size()->PrintLine();
	}

	function : native : Run(nodes : Vector) ~ Nil {
		for(i := 0; i < 150; i++;) {
			nodes->AddBack(Node->New());
		};
	}
}
