class Bar<A, B> {
	@aa : A;
	@bb : B;

	New(bb : B) {
		@bb := bb;
	}
	
	method : public : Set(a : A) ~ Nil {
		@aa := a;
	}
	
	method : public : Get() ~ B {
		return @bb;
	}

}
