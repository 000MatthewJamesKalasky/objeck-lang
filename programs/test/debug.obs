use System.IO.File;
use Collection;

class Abbreviations  {
	function : Main(args : String[]) ~ Nil {
		if(args->Size() = 1) {
			Go(args[0]);
		};
	}

	function : Go(file : String) ~ Nil {
		reader := FileReader->New(file);

		line := reader->ReadString();
		while(line <> Nil) {
			days := line->Split(" ");
days->Size()->PrintLine();
		};


		leaving {
			reader->Close();
		};
	}
}
