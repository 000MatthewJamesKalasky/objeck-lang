# TODO: use 'Compare' internally
class Bar<A : Compare, B> {
	@aa : A;
	@bb : B;

	New(bb : B) {
		@bb := bb;
	}
	
	method : public : Set(a : A) ~ Nil {
		@aa := a;
		x := A->New[3];
		x[0] := String->New("Hello");
		# x[0]->PrintLine();
		y := x[0]->As(String);
		y->PrintLine();
		x[1] := x[0];
	}
	
	method : public : Get() ~ B {
		return @bb;
	}

}
