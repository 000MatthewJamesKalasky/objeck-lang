use System.IO.File;

class Test {
	function : Main(args : String[]) ~ Nil {
		reader := FileReader->New("C:\\Users\\objec\\Desktop\\39_tiling\\lazy.map");
		leaving {
			reader->Close();
		};

		numbers := Int->New[16, 12];
		dims := numbers->Size();
		width := dims[0];
		height := dims[1];

		line := reader->ReadString();			
		for(i := 0; i < height; i +=1;) {
			tokens := line->Split(" ");
			for(j := 0; j < width; j +=1;) {
				number := tokens[j]->ToInt();
				number->PrintLine();
			};
			
			line := reader->ReadString();
		};
	}
}