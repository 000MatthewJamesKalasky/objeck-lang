use Collection.Generic;

class Foo {
	function : Main(args : String[]) ~ Nil {
#		(89 - (89 >> 2))->PrintLine();
#		(347 - (347 >> 2))->PrintLine();
#		(2767 - (2767 >> 2))->PrintLine();
#		(10949 - (10949 >> 2))->PrintLine();

		each(i : 12000) {
			load := (i + (i >> 2));
			if(i = 64) {
				"Small: {$load}"->PrintLine();
			}
			else if(i = 256) {
				"Med: {$load}"->PrintLine();
			}
			else if(i = 2048) {
				"Large: {$load}"->PrintLine();
			}
			else if(i = 8192) {
				"Ex-Large: {$load}"->PrintLine();
			};
		};
	}
}