use Data.JSON;

class Foo {
	function : Main(args : String[]) ~ Nil {
		root := JSONElement->New(JSONType->OBJECT);
        capabilities := root->AddChild("capabilities");
        capabilities->Insert("resolveProvider", true);
        completionProvider := root->AddChild("completionProvider");
        completionProvider->Insert("textDocumentSync", "TextDocumentSyncKind.Incremental");

        root->ToString()->PrintLine();
    }
}
