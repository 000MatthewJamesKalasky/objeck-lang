use System.IO.File;
use Data.CSV;

class CsvData {
	function : Main(args : String[]) ~ Nil {
		if(args->Size() > 0) {
			file_name := args[0];
			table := CsvTable->New(FileReader->ReadFile(file_name));
			if(table->IsParsed()) {
				"Parsed!"->PrintLine();
			}
			else {
				"Unable to Parse!"->ErrorLine();
			};
		};
	}
}