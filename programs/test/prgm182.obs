class Test {
	function : Main(args : String[]) ~ Nil {
		tape := "-3+2+-1";
		if(args->Size() = 1) {
			tape := args[0];
		};

		acum := 0; 
		oper := '\0';
		negate := false;
		each(i : tape) {
			char := tape->Get(i);
			
			next_char := '\0';
			if(i + 1 < tape->Size()) {
				next_char := tape->Get(i + 1);
			};

			if(char->IsDigit()) {
				value := (char - '0')->As(Int);
				select(oper) {
					label '+': {
						acum += value;
					}

					label '-': {
						acum -= value;
					}

					label '*': {
						acum *= value;
					}

					label '/': {
						acum /= value;
					}

					other  {
						acum := negate ? -value : value;
					}
				};
				oper := '\0';
			}
			else {
				select(char) {
					label '+':
					label '-':
					label '*':
					label '/': {
						if((char = '-' & i = 0) | next_char = '-') {
							negate := true;
						}
						else {
							oper := char;
							negate := false;
						};
					}
				};
			};
		};

		acum->PrintLine();
	}
}
