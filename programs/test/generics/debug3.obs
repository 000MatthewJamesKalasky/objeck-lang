#~
	testing:
		* arrays
~#

class Test {
	function : Main(args : String[]) ~ Nil {
		b : Bar[] := Bar->New[3];
#~		
		b[0] := Bar->New("Foo")<String>;
		b[0] := Bar->New(IntHolder->New(13))<IntHolder>;
~#		
		b[1] := Bar->New()<IntHolder>;
		b[1]->Set(IntHolder->New(225));
		b[1]->Get()->PrintLine();
	}
}

class Bar<V> {
	@v : V;
	
	New() {}

	New(v : V) {
		@v := v;
	}

	function : Instance() ~ Bar {
		return Bar->New()<V>;
	}
	
	method : public : Set(v : V) ~ Nil {
		@v := v;
	}
	
	method : public : Get() ~ V {
		return @v;
	}
}