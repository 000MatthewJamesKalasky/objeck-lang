#~
	testing:
		* arrays
~#

class Test {
	function : Main(args : String[]) ~ Nil {
		b : Bar[] := Bar->New[3];
		b[0] := Bar->Instance()<String>;
		b[1] := IntHolder->New();
	}
}

class Bar<V> {
	@v : V;
	
	New() {}

	New(v : V) {
		@v := v;
	}

	function : Instance() ~ Bar {
		return Bar->New()<V>;
	}
	
	method : public : Set(v : V) ~ Nil {
		@v := v;
	}
	
	method : public : Get() ~ V {
		return @v;
	}
}