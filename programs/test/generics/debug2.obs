# validate generic arrays

class Test {
	function : Main(args : String[]) ~ Nil {
		b : Bar := Bar->New()<Foo,String>; # check to make sure Foo implements Mock interface

		f := Foo->New();
#		b->Set(f); # needs type erasure
	}
}

class Bar<V : Mock, S> {
	@v : V;
	@n : Float;

	New() {
	}
	
	method : public : Set(v : V) ~ Nil {
		@v := v;
		v->DoIt()->PrintLine();
	}
	
	method : public : Get() ~ V {
		return @v;
	}
}

class Foo implements Mock {
	New() {
	}

	method : public : DoIt() ~ Int {
		return 13;
	}

}

interface Mock {
	method : virtual : public : DoIt() ~ Int;
}