# ensure constructor has all generics defined

class Test {
	function : Main(args : String[]) ~ Nil {
		b : Bar := Bar->New() <Foo>;
	}
}

class Bar<V> {
	@v : V;
	@n : Float;

	New() {
	}
	
	method : public : Set(v : V) ~ Nil {
		@v := v;
	}
	
	method : public : Get() ~ V {
		return @v;
	}
}

