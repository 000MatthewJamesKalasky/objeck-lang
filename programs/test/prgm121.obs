class Test {
	function : Main(args : String[]) ~ Nil {
		input := [
			[0, 8, 0]
			[0, 8, 8]
			[0, 0, 0]];

		Show(input);
		output := ShiftRight(input, 1);
		Show(output);

		#~
		7 4 1
		8 5 2
		9 6 3
		~#
	}

	function : ShiftRight(input : Int[,], iters : Int) ~ Int[,] {
		output := Int->New[3,3];
		for(i := 0; i < 3; i += 1;) {
			k := 2;
			for(j := 0; j < 3; j += 1;) {
				output[i,j] := input[k,i];
				k -= 1;
			};
		};

		return output;
	}

	function : Show(matrix : Int[,]) ~ Nil {
		for(i := 0; i < 3; i += 1;) {
			for(j := 0; j < 3; j += 1;) {
				value := matrix[i,j];
				"{$value} "->Print();
				k -= 1;
			};
			"\n"->Print();
		};

		"---"->PrintLine();		
	}
}	
