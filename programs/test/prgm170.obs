use Data.JSON, System.IO.File;

class JsonText  {
	function : Main(args : String[]) ~ Nil {
		if(args->Size() = 1) {
			doc := FileReader->ReadFile(args[0]);
			parser := JsonParser->New(doc);
			if(parser->Parse()) {
				"Parsed."->PrintLine();
				parser->GetRoot()->FindElements("productFirmware/id")->ToString()->PrintLine();
			}
			else {
				"Unable to parse"->PrintLine();
			};
		};
	}
}