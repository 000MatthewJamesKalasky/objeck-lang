class Test<H, K> aliases [
		foo := (x : IntHolder) ~ IntHolder),
		bar := () ~ Float] {
	function : Main(args : String[]) ~ Nil {
		foo : ;

		values := CompareVector->New()<IntHolder>;
		values->AddBack(3);
		values->AddBack(7);
		values->AddBack(13);

		applied := values->Apply(Test->Alias(foo) => x * x)<IntHolder>->Apply(foo => x - 5)<IntHolder>;
		each(i : applied) {
			applied->Get(i)->Get()->PrintLine();
		};
	}
}

