use Data.JSON;
use Collection.Generic;

class Test {
	function : Main(args : String[]) ~ Nil {
		builder := JsonBuilder->New();
		builder->PushArray();
		each(i : 3) {
			builder->PushObject();
			builder->Insert("name", i + 1)->PrintLine();
			builder->Pop();
		};
		builder->Pop()->ToString()->PrintLine();
	}
}