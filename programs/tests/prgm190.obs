class Test {
    function : Main(args : String[]) ~ Nil {
        pairs :=  "fname=Jim+Jon&lname=James&Mary=a%20b"->Split('&');
        each(i : pairs) {
            pairs[i] := Web.HTTP.Url->Decode(pairs[i])->ReplaceAll('+', ' ');
        };

        each(i : pairs) {
            pairs[i]->PrintLine();
        };
    }
}