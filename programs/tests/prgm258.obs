class Test {
	function : Main(args : String[]) ~ Nil {
		x1 := 3.0; x2 := 4.0;
		y1 := 4.0; y2 := 3.0;
		PointDistance(x1, y1, x2, y2)->PrintLine();

		x := [3.0, 4.0];
		y := [4.0, 3.0];
		NDistance(x, y)->PrintLine();
	}

	function : PointDistance(x1 : Float, y1 : Float, x2 : Float, y2 : Float) ~ Float {
		return Float->Sqrt( (y2 - y1) * (y2 - y1) + (x2 - x1) * (x2 - x1) );
	}

	function : NDistance(a : Float[], b : Float[]) ~ Float {
		if(a->Size() <> b->Size()) {
			return 0.0;
		};

		total := 0.0;
		
		each(i : a) {
			diff := b[i] - a[i];
			total += diff * diff;
		};

		return total->Sqrt();
	}
}