use Collection;

class Hello {
	function : Main(args : String[]) ~ Nil {
		v := Vector->New();
		v->AddBack(IntRef->New(7));
		v->AddBack(IntRef->New(13));
		Run(v);
	}

	function : Run(v : Vector) ~ Nil {
		l := v->Get(0)->As(IntRef);
		r := v->Get(1)->As(IntRef);
		(l->Get() + r->Get())->PrintLine();
	}
}
