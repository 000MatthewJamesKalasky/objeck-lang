class Test {
	function : Main(args : String[]) ~ Nil {
       a := 5; b := 27;

		Standard->Print("a&b = ")->PrintLine(a and b);
		Standard->Print("a&b = ")->PrintLine(a or b);
		Standard->Print("a&b = ")->PrintLine(a xor b);
		Standard->Print("~a = ")->PrintLine(not a);
	}
}

