use Web.HTTP, Collection;

class Test {
	function : Main(args : String[]) ~ Nil {
		a := [
			[1., 9., 7.]
			[8., 1., 2.]];
		b := [
			[3., 2., 1., 5.]
			[5., 4., 7., 3.]
			[6., 9., 6., 8.]];
		System.ML.Matrix2D->DotProduct(a, b)->ToString();
		# "---"->PrintLine();

		a := [
			[1., 5.]
			[4., 8.]
			[7., 9.]];
		System.ML.Matrix2D->Transpose(a);
		# "---"->PrintLine();

		m1 := [
			[1., 1., 1.]
			[-1., 3., 2.]
			[2., 2., 1.]];
		m2 := [
			[5.]
			[2.]
			[1.]];
		System.ML.Matrix2D->DotProduct(System.ML.Matrix2D->Inverse(m1), m2)->PrintLine();
		"---"->PrintLine();
	}
}