use Collection, API.Google.Gemini, System.IO.Filesystem;

class Test {
	function : Main(args : String[]) ~ Nil {
		key := EndPoint->GetApiKey();
		
		models := Model->ListModels(key);
		each(model in models) {
			model->GetName()->PrintLine();
		};

		content := Content->New("user")->AddPart(TextPart->New("What number is this image showing?"))->AddPart(BinaryPart->New(FileReader->ReadBinaryFile("thirdteen.png"), "image/png"));
		model := Model->Create("models/gemini-pro-vision", key);
		model->GenerateContent(content, Nil, Nil, key);
	}
}