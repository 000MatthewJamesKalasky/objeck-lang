use System.ML;
	
class Test {
	function : Main(args : String[]) ~ Nil {
		if(args->Size() > 0) {
			input := DecisionTree->LoadMatrix(args[0]);
			input->ToString()->PrintLine();
			
			tree := DecisionTree->New(input);
			decisions := tree->Decide();
			each(decision := decisions) {
				decision->ToString()->PrintLine();
			};
		};
	}
}