use System.ML;

class Test {
	function : Main(args : String[]) ~ Nil {
		if(args->Size() > 0) {
			input := DecisionTree->LoadMatrix(args[0]);
			input->ToString()->PrintLine();
			
			"---"->PrintLine();

			tree := DecisionTree->New(input);			
			split := tree->Split(input);
			split->ToString()->PrintLine();
			DecisionTree->Matches(2,split)->PrintLine();
		};
	}
}