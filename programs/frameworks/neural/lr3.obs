use System.ML;

class MultiRegression {
	function : Main(args : String[]) ~ Nil {
		a := [
			[1.,  1., 3.]
			[-1., 4., 1.]
			[0.,  2., -2.]];

		b := [
			[1.]
			[0.]
			[-1.]];

		Matrix2D->Dot(a,b)->PrintLine();
		"---"->PrintLine();

		Matrix2D->Transpose(a)->PrintLine();
		"---"->PrintLine();

		a := [
			[1., 2., 3.]
			[4., 5., 6.]];

		b := [
			[10., 11.]
			[20., 21.]
			[30., 31.]];
		Matrix2D->Multiple(a,b)->PrintLine();
		"---"->PrintLine();
	}
}