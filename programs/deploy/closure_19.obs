use Collection;

class Capture {
	function : Main(args : String[]) ~ Nil {
		 funcs := Vector->New()<FuncRef<IntRef>>;
		 
		 for(i := 0; i < 10; i += 1;) {
		 	funcs->AddBack(FuncRef->New(\() ~ IntRef : () => System.Math.Routine->Factorial(i) * funcs->Size())<IntRef>);
		 };
		 
		 each(i : funcs) {
		 	value := funcs->Get(i);
			func := value->Get();
		 	func()->Get()->PrintLine();
		 };
	}
}
