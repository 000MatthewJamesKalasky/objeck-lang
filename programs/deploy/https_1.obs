use Web.HTTP;

class HttpsTest {
	function : Main(args : String[]) ~ Nil {
		html := HttpsClient->New()->GetAll("https://www.nasa.gov");
		if(html = Nil) {
			"--- Unable to fetch URL ---"->ErrorLine();
		}
		else {
			html->PrintLine();
		};
	}
}
