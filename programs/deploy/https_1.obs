use Collection.Generic;
use Web.HTTP;

class HttpsTest {
	function : Main(args : String[]) ~ Nil {
		if(args->Size() = 1) {
			html := HttpsClient->GetAll(args[0]);
			if(html = Nil) {
				"--- Unable to fetch URL ---"->ErrorLine();
			}
			else {
				html->PrintLine();
			};
		};
	}
}
