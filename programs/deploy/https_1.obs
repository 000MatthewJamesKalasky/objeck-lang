use Web.HTTP;

class HttpsTest {
	function : Main(args : String[]) ~ Nil {
		client := HttpsClient->New();
		lines := client->Get("https://www.google.com");
		each(i : lines) {
			lines->Get(i)->As(String)->Print();
		};
		IO.Console->PrintLine();
	}
}
